LLM_PROVIDER=openai

# OpenAI
OPENAI_API_KEY=
OPENAI_MODEL=gpt-4o-mini
OPENAI_MODEL_FAST=gpt-4o-mini-high
# Specific override for KnowledgeAgent (optional). Falls back to OPENAI_MODEL_FAST or OPENAI_MODEL
OPENAI_MODEL_KNOWLEDGE=
OPENAI_EMBED_MODEL=text-embedding-3-small

# Embeddings (fallback if not using OpenAI)
EMBEDDINGS_MODEL=sentence-transformers/all-MiniLM-L6-v2

# Web search (optional)
TAVILY_API_KEY=
FIRECRAWL_API_KEY=

# LangSmith / LangChain tracing (optional)
LANGSMITH_API_KEY=
LANGCHAIN_TRACING_V2=true
LANGCHAIN_PROJECT=PS-CloudWalk

# Neo4j (recommended: AuraDB Free)
NEO4J_URI=neo4j+s://<your-instance>.databases.neo4j.io
NEO4J_USERNAME=neo4j
NEO4J_PASSWORD=********
NEO4J_DATABASE=neo4j
AURA_INSTANCEID=
AURA_INSTANCENAME=

# App
APP_ENV=dev
LOG_LEVEL=INFO

# Supabase / Postgres (LangGraph checkpointer)
DATABASE_URL=

# Rate limiting
RATE_LIMIT_PER_MINUTE=60

# Slack (CustomAgent)
SLACK_BOT_TOKEN=
SLACK_DEFAULT_CHANNEL=#support

# Retrieval knobs
RAG_VECTOR_K=3
RAG_VECTOR_K_FEES=2
RAG_GRAPH_WEIGHT=0.7
RAG_VECTOR_WEIGHT=0.3
RAG_MAX_CONTEXT_CHARS=1800

